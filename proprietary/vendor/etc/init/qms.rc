
on post-fs-data
      mkdir /data/vendor/qms 0700 radio radio
      mkdir /data/vendor/qms_logs 0700 radio radio
      mkdir /data/vendor/qms_logs/main 0700 radio radio
      mkdir /data/vendor/qms_logs/other 0700 radio radio
      mkdir /data/vendor/qms_logs/config 0700 radio radio

on property:sys.boot_completed=1
    copy /vendor/etc/SNS_official_license.pfm /mnt/vendor/persist/data/pfm/licenses/SNS_official_license.pfm
    chmod 0644 /mnt/vendor/persist/data/pfm/licenses/SNS_official_license.pfm
    chown system system /mnt/vendor/persist/data/pfm/licenses/SNS_official_license.pfm
    copy /vendor/etc/SDA_official_license.pfm /mnt/vendor/persist/data/pfm/licenses/SDA_official_license.pfm
    chmod 0644 /mnt/vendor/persist/data/pfm/licenses/SDA_official_license.pfm
    chown system system /mnt/vendor/persist/data/pfm/licenses/SDA_official_license.pfm

service vendor.qms /vendor/bin/qms
     class main
     group radio wakelock wifi inet net_admin
